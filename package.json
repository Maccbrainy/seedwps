  },
  "homepage": "https://github.com/Maccbrainy/seedwps#readme",
  "dependencies": {},
  "devDependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cross-env": "^7.0.2",
    "jquery": "^3.5.0",
    "laravel-mix": "^5.0.4",
    "lodash": "^4.17.15",
    "popper.js": "^1.16.1",
